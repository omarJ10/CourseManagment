        <link rel="stylesheet" href="css/style.css">
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% include 'extra/header.html.twig' %}
        {% block body %}{% endblock %}
